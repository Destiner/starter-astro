{
  "name": "starter-astro",
  "version": "1.0.0",
  "description": "(Almost) unopinionated, unstyled Astro starter, useful for quick prototypes and theme creation",
  "author": "Timur Badretdinov <destinerx@gmail.com>",
  "keywords": [
    "astro",
    "template"
  ],
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint",
    "lint:prettier": "prettier -c \"src/**/*.{json,js,ts,vue}\"",
    "lint:eslint": "eslint \"src/**/*.{js,ts,vue}\"",
    "lint:stylelint": "stylelint \"src/**/*.{css,vue}\"",
    "typecheck": "npx tsc --noEmit && npx vue-tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Destiner/starter-astro.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@astrojs/sitemap": "^0.1.0",
    "@astrojs/vue": "^0.1.5",
    "@types/luxon": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.27.0",
    "astro": "^1.0.0-beta.40",
    "astro-analytics": "^2.1.1",
    "eslint": "^8.17.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.6.0",
    "luxon": "^2.4.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.5",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vue": "^3.2.36",
    "vue-tsc": "^0.36.1"
  }
}
